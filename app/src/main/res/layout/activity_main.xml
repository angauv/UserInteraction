<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.server.userinteraction.MainActivity">
<!--
    <Button android:id="@+id/button_a"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/button_a_txt"
        android:layout_alignParentLeft="true" />
    <Button android:id="@+id/button_b"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/button_b_txt"
        android:layout_toRightOf="@+id/button_a" />
-->
    <Button android:id="@+id/button_left"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/button_left_txt"
        android:layout_alignParentLeft="true"
        android:onClick="leftClick" />
    <Button android:id="@+id/button_right"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/button_right_txt"
        android:layout_toRightOf="@+id/button_left"
        android:onClick="rightClick" />
    <Button android:id="@+id/button_go"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/button_go_txt"
        android:layout_toRightOf="@+id/button_right"
        android:onClick="goClick" />

    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_below="@+id/button_b"
        android:contentDescription="@string/hello_world" />


</RelativeLayout>
